{
    "name": "imiphp/imi-queue",
    "type": "library",
    "license": "MIT",
    "description": "imi 框架的队列组件，使用 Redis 作为持久化",
    "require": {
        
    },
    "require-dev": {
        "yurunsoft/imi": "dev-dev",
        "phpunit/phpunit": ">=7",
        "yurunsoft/ide-helper": "~1.0"
    },
    "autoload": {
        "psr-4" : {
            "Imi\\Queue\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "ImiApp\\" : "example/",
            "Imi\\Queue\\Test\\" : "tests/"
        }
    },
    "scripts": {
      "test": "./tests/run",
      "install-test": "php --ri swoole && composer install composer test"
    },
    "extra": {
        "ide-helper": {
            "list": [
                "swoole",
                "redis"
            ]
        }
    }
}
